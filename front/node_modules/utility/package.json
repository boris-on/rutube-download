{
  "name": "utility",
  "version": "1.6.0",
  "description": "A collection of useful utilities.",
  "main": "lib/utility.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "mocha -R spec -t 5000 test/*.test.js",
    "test-cov": "istanbul cover node_modules/.bin/_mocha -- -t 5000 test/*.test.js",
